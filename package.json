{
  "name": "isomorphic-typescript-react-redux-starter",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "prod:build-server": "webpack --config webpack.server.js -p",
    "prod:build-client": "webpack --config webpack.client.js -p",
    "build": "npm-run-all --parallel prod:*",
    "start": "node build/bundle.js",
    "lint": "tslint 'src/**/*.ts?(x)' --format stylish -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "prepush": "npm-run-all --parallel test:coverage lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "await-to-js": "~2.0.1",
    "axios": "0.16.2",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0",
    "styled-components": "~2.2.4",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "@types/axios": "~0.14.0",
    "@types/enzyme": "~3.1.5",
    "@types/express": "~4.0.39",
    "@types/jest": "~21.1.8",
    "@types/react": "~16.0.25",
    "@types/react-dom": "~16.0.3",
    "@types/react-helmet": "~5.0.3",
    "@types/react-redux": "~5.0.13",
    "@types/react-router-config": "~1.0.4",
    "@types/react-router-dom": "~4.2.2",
    "@types/redux": "~3.6.0",
    "@types/redux-thunk": "~2.1.0",
    "@types/serialize-javascript": "~1.3.2",
    "awesome-typescript-loader": "~3.4.0",
    "enzyme": "~3.2.0",
    "enzyme-adapter-react-16": "~1.1.0",
    "file-loader": "~1.1.5",
    "husky": "~0.14.3",
    "image-webpack-loader": "~3.4.2",
    "jest": "~21.2.1",
    "jest-styled-components": "~4.9.0",
    "ts-jest": "~21.2.4",
    "tslint": "~5.8.0",
    "tslint-config-airbnb": "~5.4.2",
    "tslint-loader": "~3.5.3",
    "tslint-react": "~3.2.0",
    "typescript": "~2.6.1",
    "url-loader": "~0.6.2",
    "webpack-merge": "~4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "mapCoverage": true,
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/global.d.ts",
      "/src/types.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "setupFiles": [
      "./test/setup.js"
    ]
  }
}
